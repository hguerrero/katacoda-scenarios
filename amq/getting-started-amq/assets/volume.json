{
    "apiVersion": "v1",
    "kind": "List",
    "items": [
        {
            "apiVersion": "v1",
            "kind": "PersistentVolume",
            "metadata": {
                "name": "pv-01"
            },
            "spec": {
                "accessModes": [
                    "ReadWriteOnce",
                    "ReadWriteMany",
                    "ReadOnlyMany"
                ],
                "capacity": {
                    "storage": "1Gi"
                },
                "hostPath": {
                    "path": "/data/pv-01"
                },
                "persistentVolumeReclaimPolicy": "Recycle"
            }
        }
    ]
}